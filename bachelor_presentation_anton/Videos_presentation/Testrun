
#this is just to test if tossing is physically possible. (This uses Toss-v0 and grabs a ball on the table and tosses it)

import gym

env = gym.make("FetchToss-v0")

for i in range(10):

 env.reset()

 env.render()
 env.step([0,0,-1,1])
 env.render()
 env.step([0,0,-1,1])
 env.render()
 env.step([0,0,-1,1])
 env.render()
 
 env.step([0,0,0,-1])
 env.render()
 env.step([0,0,0,-1])
 env.render()
 env.step([0,0,0,-1])
 env.render() 
 
 env.step([0,0,1,-1])
 env.render()
 env.step([0,0,1,-1])
 env.render()
 env.step([0,0,1,-1])
 env.render()
 env.step([0,0,1,-1])
 env.render()
 
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render() 
 
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render() 
 
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render() 
 
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render() 
 
 env.close()

