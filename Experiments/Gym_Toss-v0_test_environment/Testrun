import gym

env = gym.make("FetchToss-v4")

for i in range(10):

 env.reset()

 env.render()
 env.step([1,0,0,0])
 env.render()
 env.step([1,0,0,0])
 env.render()
 env.step([0,0,-1,1])
 env.render()
 env.step([0,0,-1,1])
 env.render()
 env.step([0,0,-1,1])
 env.render()
 
 env.step([0,0,0,-1])
 env.render()
 env.step([0,0,0,-1])
 env.render()
 env.step([0,0,0,-1])
 env.render() 
 
 env.step([0,0,1,-1])
 env.render()
 env.step([0,0,1,-1])
 env.render()
 env.step([0,0,1,-1])
 env.render()
 env.step([0,0,1,-1])
 env.render()
 
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render()
 env.step([-0.5,0,-0.5,-1])
 env.render() 
 
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render()
 env.step([1,0,1,-1])
 env.render() 
 
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render()
 env.step([1,0,1,0.1])
 env.render() 
 
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render()
 env.step(env.action_space.sample())
 env.render() 
 
 env.close()

